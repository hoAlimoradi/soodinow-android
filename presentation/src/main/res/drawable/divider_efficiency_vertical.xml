<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="7dp"
        android:left="-1dp"
        android:right="-1dp"
        android:top="7dp">

        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="#d1efbc"/>

            <solid android:color="@android:color/transparent" />
            <size
                android:width="5dp"
                android:height="5dp" />
            <padding android:right="@dimen/margin_s" android:left="@dimen/margin_s"/>

        </shape>
    </item>
</layer-list>